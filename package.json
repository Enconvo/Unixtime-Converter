{
  "$schema": "https://enconvo.com/schemas/extension.json",
  "name": "unixtime",
  "version": "0.0.10",
  "description": "converter between unix time and human readable time.",
  "title": "UnixTime Converter",
  "icon": "icon.png",
  "author": "EnconvoAI",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "type": "module",
  "commands": [
    {
      "name": "unixtime",
      "icon": "icon.png",
      "title": "Unix Time",
      "description": "Convert between unix time and human readable time.",
      "mode": "no-view",
      "parameters": {
        "type": "object",
        "properties": {
          "input_text": {
            "type": "string",
            "description": "The text to be translated"
          },
          "selection_text": {
            "type": "string",
            "description": "The selection text to be translated"
          }
        }
      },
      "preferences": []
    }
  ],
  "dependencies": {
    "@enconvo/api": "^0.1.161"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "19.0.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\"",
    "build": "enconvo",
    "dev": "enconvo --dev",
    "publish": "enconvo --publish"
  }
}
